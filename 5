using System;

class Program
{
    static void Main(string[] args)
    {
        Console.WriteLine("Podaj dwie liczby:");
        int a = int.Parse(Console.ReadLine());
        int b = int.Parse(Console.ReadLine());

        int wieksza = (a > b) ? a : b;

        Console.WriteLine("WiÄ™ksza liczba: " + wieksza);

        Console.ReadLine();
    }
}